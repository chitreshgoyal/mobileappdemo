<div class="container well"> 
<div class="span3">
    <h3>Sign Up</h3>
	<%= form_for(@user,:html => {:class=> "form-horizontal" }) do |f| %>	
		<%= render :partial => "form",  :object => f, :locals => { :user => @user } %>
		<%= f.submit "Register", :class=> "btn btn-primary pull-right" %>
		<div class="clearfix"></div>
	<% end %>
</div>
</div>