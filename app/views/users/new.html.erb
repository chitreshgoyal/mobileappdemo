<div class="container"> 
<div class="span3">
    <h2>Register</h2>

	<%= form_for @user do |f| %>
		<%= render :partial => "form",  :object => f, :locals => { :user => @user } %>
		<%= f.submit "Register", :class=> "btn btn-primary pull-right" %>
		<div class="clearfix"></div>
	<% end %>
</div>
</div>